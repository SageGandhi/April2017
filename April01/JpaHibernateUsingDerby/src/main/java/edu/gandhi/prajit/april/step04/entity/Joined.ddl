CREATE TABLE ITEM (ID BIGINT NOT NULL, DiscriminatorType VARCHAR(31), DESCRIPTION VARCHAR(3000), TITLE VARCHAR(100), unit_cost FLOAT, PRIMARY KEY (ID))
CREATE TABLE BOOK (ID BIGINT NOT NULL, ISBN VARCHAR(15), nb_of_pages INTEGER, publication_date DATE, PRIMARY KEY (ID))
CREATE TABLE COMPACTDISCVERSION01 (ID BIGINT NOT NULL, GENRE VARCHAR(255), TOTALDURATION FLOAT, PRIMARY KEY (ID))
CREATE TABLE MUSICIAN (ID BIGINT NOT NULL, BIO VARCHAR(5000), date_of_birth DATE, first_name VARCHAR(50), last_name VARCHAR(50), preferred_instrument VARCHAR(255), MUSICIANS_ID BIGINT, PRIMARY KEY (ID))
ALTER TABLE BOOK ADD CONSTRAINT FK_BOOK_ID FOREIGN KEY (ID) REFERENCES ITEM (ID)
ALTER TABLE COMPACTDISCVERSION01 ADD CONSTRAINT CMPCTDSCVERSION01D FOREIGN KEY (ID) REFERENCES ITEM (ID)
ALTER TABLE MUSICIAN ADD CONSTRAINT MSICIANMUSICIANSID FOREIGN KEY (MUSICIANS_ID) REFERENCES ITEM (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
