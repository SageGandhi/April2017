CREATE TABLE CompactDiscDetails (ID BIGINT NOT NULL, DESCRIPTION VARCHAR(5000), GENRE VARCHAR(50), TITLE VARCHAR(255) NOT NULL, PlayTime FLOAT, Price FLOAT, PRIMARY KEY (ID))
CREATE TABLE APP.BookDetails (ID BIGINT NOT NULL, DESCRIPTION VARCHAR(255), ISBN VARCHAR(255), NUMBEROFPAGE INTEGER, TITLE VARCHAR(255), UNITCOST FLOAT, PRIMARY KEY (ID))
CREATE TABLE APP.AuthorDetails (ID BIGINT NOT NULL, BIO VARCHAR(1000), DATEOFBIRTH DATE, AuthorFirstName VARCHAR(100) NOT NULL, LANGUAGE VARCHAR(255), AuthorLastName VARCHAR(100), PRIMARY KEY (ID))
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(15), PRIMARY KEY (SEQ_NAME))
